#include<stdio.h>
#include<string.h>
int main()
{
    char s[101];
    scanf("%s", s);
    for(int i=0; i < strlen(s); i++)
    {

        if(s[i] <= 'Z' && s[i] >= 'A')
            s[i] += 32;
        if(s[0] >= 'a' && s[0] <= 'z' )
            s[0] -= 32;
    }
    printf("%s", s);
    return 0; 
    // for(int i=0; i < strlen(s); i++)
    // {
    //     if(s[i] >='0' && s[i] <='9')
    //         break;
    // }
}
