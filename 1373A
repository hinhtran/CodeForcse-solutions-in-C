#include<stdio.h>

int main()
{
    int t;
    long long int a,b,c,f,s;
    scanf("%d", &t);
    while(t--)
    {
        f = -1;
        s = -1;
        scanf("%lld %lld %lld",&a,&b,&c);
        if(a<c)
            f = 1;
        if(a * b > c)
            s = b;
        printf("%lld %lld\n", f, s);
    }
}
