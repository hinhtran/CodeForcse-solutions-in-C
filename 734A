#include<stdio.h>
#include<string.h>

int main()
{
    int n, l1 = 0, l2 = 0;
    scanf("%d", &n);
    char s[n];
    scanf("%s", s);
    for(int i = 0; i < n; i++)
    {
        if(s[i] == 'A')
            l1++;
        if(s[i] == 'D')
            l2++;
    }
    if(l1 > l2)
        printf("Anton\n");
    else if(l1 < l2)
        printf("Danik\n");
    else
        printf("Friendship\n");
    return 0;
}
