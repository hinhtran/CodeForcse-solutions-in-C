#include<stdio.h>

int main()
{
    long long int t;
    scanf("%lld", &t);
    while(t--)
    {
        long long int n,m,i;
        scanf("%lld", &n);
        m = (n - 1) / 4;
        for (int i=0; i < n - m -1; i++)
        {
            printf("9");
        }
        for (int i=0; i < m + 1; i++)
        {
            printf("8");
        }
        printf("\n");
    }
    return 0;
}
