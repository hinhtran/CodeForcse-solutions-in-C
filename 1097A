#include<stdio.h>

int main()
{
    int c = 0, i;
    char s[20], s1[20];
    scanf("%s", s);
    for(i = 0; i < 5; i++)
    {
        scanf("%s", s1);
        if((s[0]==s1[0])||(s[1]==s1[1]))
            c++;
    }
    if(c>0)
        printf("YES\n");
    else
        printf("NO\n");
    return 0;
}
