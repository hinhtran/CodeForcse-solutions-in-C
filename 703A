#include<stdio.h>

int main()
{
    int n, a, b, cnt_mishka = 0, cnt_chris = 0;
    scanf("%d", &n);
    for(int i = 0; i < n; i++)
    {
        scanf("%d %d", &a, &b);
        if(a > b)
        {
            cnt_mishka ++;
        }
        else
        {
            cnt_chris ++;
        }
    }
    if(cnt_mishka > cnt_chris)
    {
        printf("Mishaka\n");
    }
    else if(cnt_mishka < cnt_chris)
    {
        printf("Chiris\n");
    }
    else
    {
        printf("friendship is magic!^^\n");
    }
    return 0;
}
