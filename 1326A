#include<stdio.h>
int main()
{
    int t,n,i;
    scanf("%d", &t);
    while(t--)
    {
        scanf("%d", &n);
        if(n == 1)
            printf("-1");
        else
        {
            printf("5");
            for(int i = 1; i< n; i++)
                //printf("9");
                printf("7");
            //printf("4");
        }
        printf("\n");
    }
    return 0;
}
