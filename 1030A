#include<stdio.h>

int main()
{
    int t, arr[101], c = 0, c1 = 0;
    scanf("%d", &t);
    for(int i = 0; i < t; i++)
    {
        scanf("%d", &arr[i]);
    }
    for(int i = 0; i < t; i++)
    {
        if(arr[i] == 1)
            c++;
        else if(arr[i] == 0)
            c1++;
        
    }
    if(c >= 1)
        printf("HARD\n");
    else
        printf("EASY\n");

    return 0;
}
